@model List<TelecomWebApp.Models.UsageCurrMonth>

<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="~/css/PlanUsage.css" asp-append-version="true" />
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Usage in current month is:</title>
</head>
<body>
    <div>
        <table>
            <tr>
                <th>Data</th>
                <th>Minutes</th>
                <th>SMS</th>
            </tr>
            @foreach (var u in Model)
            {
                <tr>
                    <td>@u.data_consumption</td>
                    <td>@u.minutes_used</td>
                    <td>@u.SMS_sent</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
