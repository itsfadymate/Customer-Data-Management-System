@{
    ViewData["Title"] = "Search Account Usage Plan";
}

<h1>Search Account Usage Plan</h1>
<link rel="stylesheet" href="~/css/AdminFirstHalf.css" />


<form method="post">
    <div class="form-group">
        <label for="mobileNum">Mobile Number</label>
        <input type="text" class="form-control" id="mobileNum" name="mobileNum" maxlength="11" required />
    </div>
    <div class="form-group">
        <label for="startDate">Start Date</label>
        <input type="date" class="form-control" id="startDate" name="startDate" required />
    </div>
    <button type="submit" class="btn btn-primary">Search</button>
</form>
